{
  "name": "@k-kind/svg-generator",
  "version": "0.0.3",
  "description": "Helper library for generating svg with typescript",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "rollup --config",
    "watch": "rollup --config --watch",
    "build": "rollup --config",
    "prepublishOnly": "npm run build",
    "lint:eslint": "eslint --ext .js,.ts .",
    "lint:prettier": "prettier -c .",
    "lint": "npm run lint:eslint; npm run lint:prettier",
    "fix": "npm run lint:eslint -- --fix; npm run lint:prettier -- --write",
    "test": "jest"
  },
  "author": "k-kind",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/K-kind/svg-generator.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.45.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
